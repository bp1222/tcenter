
body {

  font-weight: normal;
  font-size: 14px;
  color: #000000;
  font-family: Tahoma, Verdana, Arial, Helvetica, sans-serif;
  background-color: #FFFFFF;
  background-color: #FBFDF6;

}

#frame {

  width: 800px;
  margin-right: auto;
  margin-left: auto;
  margin-top: 10px;
  padding: 2px;
  text-align:left;

}

.ticketbox {
  
  float: left;
  margin: 4px;
  padding: 6px;
  border: solid 1px gray;
  width: 40%;
  max-height: 80%;
  
}

.ticketcount {

  font-size: 14px;
  color: black;
  margin-bottom: 5px;
  
}

.tickets {

  border: solid 1px #000000;  
  background-color: #C6D0B5;
  
  height: auto;
  min-height: 300px;
  width: 100%;
  max-height: 80%;
  overflow: scroll;

}

.ticket {
  background-color: #EDF7DC;
  margin: 4px;
  padding: 4px;
}
tr.ticket {
  margin: 4px;
}
td.ticket:hover {
  background-color: #F7FBEF;
}

.ticketnumber {
  font-size: large;
  margin-bottom: 8px;
}
a.ticketnumber:link, a.ticketnumber:visited {
  color: #608117;
  border-bottom: 1px dotted #8A8F95;
  font-size: large;
  text-decoration: none;
}
a.ticketnumber:hover {
  border-bottom: 3px solid #C6D0B5;
  font-size: large;
  text-decoration: none;
}

.lastupdate {
  display: block; 
  padding-top: 10px;
}

.summary {
  display: block; 
  padding-top: 2px;
}

